{
    "name": "windwalker/cache",
    "type": "windwalker-package",
    "description": "Windwalker Cache package",
    "keywords": [
        "windwalker",
        "framework",
        "cache",
        "psr-6",
        "psr-16"
    ],
    "homepage": "https://github.com/ventoviro/windwalker-event",
    "license": "MIT",
    "provide": {
        "psr/event-dispatcher-implementation": "1.0.0"
    },
    "require": {
        "php": ">=8.0.0",
        "psr/cache": "^1.0",
        "psr/simple-cache": "^1.0",
        "windwalker/utilities": "dev-master"
    },
    "require-dev": {
        "windwalker/test": "dev-master"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Cache\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Cache\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    }
}
