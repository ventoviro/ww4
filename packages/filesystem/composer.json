{
    "name": "windwalker/filesystem",
    "type": "windwalker-package",
    "description": "Windwalker Filesystem package",
    "keywords": [
        "windwalker",
        "framework",
        "filesystem"
    ],
    "homepage": "https://github.com/ventoviro/windwalker-filesystem",
    "license": "MIT",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "windwalker/test": "~4.0"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Filesystem\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Filesystem\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
