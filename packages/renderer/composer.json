{
    "name": "windwalker/renderer",
    "type": "windwalker-package",
    "description": "Windwalker Renderer package",
    "keywords": [
        "windwalker",
        "framework",
        "renderer",
        "blade",
        "edge",
        ""
    ],
    "homepage": "https://github.com/ventoviro/windwalker-renderer",
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "windwalker/event": "~3.0",
        "ext-json": "*"
    },
    "require-dev": {
        "windwalker/test": "~4.0"
    },
    "suggest": {
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Renderer\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Renderer\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.5"
        }
    }
}
