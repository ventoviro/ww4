{
    "name": "windwalker/utilities",
    "type": "windwalker-package",
    "description": "Windwalker Utilities package",
    "keywords": [
        "windwalker",
        "framework",
        "utilities"
    ],
    "homepage": "https://github.com/ventoviro/windwalker-utilities",
    "license": "MIT",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "windwalker/test": "~3.0",
        "doctrine/inflector": "^1.3"
    },
    "suggest": {
        "doctrine/inflector": "Install ~1.0 to use inflector.",
        "opis/closure": "Install ^3.0 to support serializable Closure."
    },
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Windwalker\\Utilities\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Utilities\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
