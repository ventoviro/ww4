{
    "name": "windwalker/ww4",
    "description": "PHP RAD framework.",
    "homepage": "https://github.com/ventoviro/ww4",
    "license": "MIT",
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Simon Asika",
            "email": "asika32764@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "ext-json": "*",
        "ext-dom": "*",
        "psr/log": "^1.0",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/cache": "^1.0",
        "psr/simple-cache": "^1.0",
        "psr/link": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "paragonie/constant_time_encoding": "^2.3",
        "composer/semver": "^3.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "composer/ca-bundle": "^1.2",
        "relay/relay": "~2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "mockery/mockery": "^1.3",
        "symfony/var-dumper": "^5.0",
        "symfony/yaml": "^5.0",
        "laktak/hjson": "^2.1",
        "yosymfony/toml": "^1.0",
        "opis/closure": "4.x-dev||^4.0",
        "phpunit/phpunit": "^8.4||^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "swoole/ide-helper": "^4.4",
        "reactivex/rxphp": "^2.0",
        "react/event-loop": "^1.1",
        "mikey179/vfsstream": "^1.6",
        "webmozart/glob": "^4.1",
        "symfony/dom-crawler": "^5.0",
        "symfony/css-selector": "^5.0",
        "ext-openssl": "*",
        "ext-sodium": "*",
        "ext-pdo": "*",
        "jdorn/sql-formatter": "^1.2",
        "asika/sql-splitter": "^1.0",
        "doctrine/inflector": "^2.0",
        "guzzlehttp/promises": "^1.3",
        "react/promise": "^2.7",
        "masterminds/html5": "^2.7",
        "react/socket": "^1.5",
        "react/http": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "twig/twig": "^2.0",
        "illuminate/view": "^6.0||^7.0||^8.0",
        "mustache/mustache": "^2.0",
        "league/plates": "dev-master"
    },
    "suggest": {
        "symfony/yaml": "Install ~4.0 if you require YAML support.",
        "php-http/httplug": "Install ~2.0 if you require HttpClient as HttpPlug implementation.",
        "webmozart/glob": "Install ^4.0 to support Ant-like glob().",
        "ext-openssl": "Install openssl to support legacy openssl cipher.",
        "doctrine/inflector": "Install ~1.0 to use inflector.",
        "opis/closure": "Install ^3.0 to support serializable Closure.",
        "aws/aws-sdk-php": "If you want to use AWS SQS as queue service.",
        "iron-io/iron_mq": "If you want to use Iron.io as queue service.",
        "php-amqplib/php-amqplib": "If you want to use RabbitMQ as queue service.",
        "pda/pheanstalk": "If you want to use Beanstalkd queue as service.",
        "chrisboulton/php-resque": "Install <= 1.2 if you want to use PHP Resque (Redis) as queue service.",
        "reactivex/rxphp": "Support EventEmitter::observe() pattern.",
        "relay/relay": "Install ^2.0 to support PSR-15 middlewares."
    },
    "autoload": {
        "psr-4": {
            "Windwalker\\Cache\\": "packages/cache/src",
            "Windwalker\\Crypt\\": "packages/crypt/src",
            "Windwalker\\DI\\": "packages/di/src",
            "Windwalker\\DOM\\": "packages/dom/src",
            "Windwalker\\Data\\": "packages/data/src",
            "Windwalker\\Database\\": "packages/database/src",
            "Windwalker\\Edge\\": "packages/edge/src",
            "Windwalker\\Event\\": "packages/event/src",
            "Windwalker\\Filesystem\\": "packages/filesystem/src",
            "Windwalker\\Http\\": "packages/http/src",
            "Windwalker\\Promise\\": "packages/promise/src",
            "Windwalker\\Query\\": "packages/query/src",
            "Windwalker\\Queue\\": "packages/queue/src",
            "Windwalker\\Reactor\\": "packages/reactor/src",
            "Windwalker\\Renderer\\": "packages/renderer/src",
            "Windwalker\\Scalars\\": "packages/scalars/src",
            "Windwalker\\Session\\": "packages/session/src",
            "Windwalker\\Stream\\": "packages/stream/src",
            "Windwalker\\Test\\": "packages/test/src",
            "Windwalker\\Utilities\\": "packages/utilities/src",
            "Windwalker\\Validator\\": "packages/validator/src"
        },
        "files": [
            "packages/bootstrap.php",
            "packages/cache/src/bootstrap.php",
            "packages/crypt/src/bootstrap.php",
            "packages/data/src/bootstrap.php",
            "packages/database/src/bootstrap.php",
            "packages/di/src/bootstrap.php",
            "packages/dom/src/bootstrap.php",
            "packages/edge/src/bootstrap.php",
            "packages/event/src/bootstrap.php",
            "packages/filesystem/src/bootstrap.php",
            "packages/http/src/bootstrap.php",
            "packages/promise/src/bootstrap.php",
            "packages/query/src/bootstrap.php",
            "packages/queue/src/bootstrap.php",
            "packages/reactor/src/bootstrap.php",
            "packages/renderer/src/bootstrap.php",
            "packages/scalars/src/bootstrap.php",
            "packages/session/src/bootstrap.php",
            "packages/test/src/bootstrap.php",
            "packages/utilities/src/bootstrap.php",
            "packages/validator/src/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Windwalker\\Cache\\Test\\": "packages/cache/test",
            "Windwalker\\Crypt\\Test\\": "packages/crypt/test",
            "Windwalker\\DI\\Test\\": "packages/di/test",
            "Windwalker\\DOM\\Test\\": "packages/dom/test",
            "Windwalker\\Database\\Test\\": "packages/database/test",
            "Windwalker\\Edge\\Test\\": "packages/edge/test",
            "Windwalker\\Event\\Test\\": "packages/event/test",
            "Windwalker\\Filesystem\\Test\\": "packages/filesystem/test",
            "Windwalker\\Http\\Test\\": "packages/http/test",
            "Windwalker\\Promise\\Test\\": "packages/promise/test",
            "Windwalker\\Query\\Test\\": "packages/query/test",
            "Windwalker\\Queue\\Test\\": "packages/queue/test",
            "Windwalker\\Reactor\\Test\\": "packages/reactor/test",
            "Windwalker\\Renderer\\Test\\": "packages/renderer/test",
            "Windwalker\\Scalars\\Test\\": "packages/scalars/test",
            "Windwalker\\Session\\Test\\": "packages/session/test",
            "Windwalker\\Test\\Test\\": "packages/test/test",
            "Windwalker\\Utilities\\Test\\": "packages/utilities/test",
            "Windwalker\\Validator\\Test\\": "packages/validator/test"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    },
    "provide": {
        "psr/cache-implementation": "~1.0",
        "psr/http-message-implementation": "~1.0"
    },
    "replace": {
        "windwalker/application": "self.version",
        "windwalker/authentication": "self.version",
        "windwalker/authorisation": "self.version",
        "windwalker/cache": "self.version",
        "windwalker/compare": "self.version",
        "windwalker/console": "self.version",
        "windwalker/controller": "self.version",
        "windwalker/crypt": "self.version",
        "windwalker/data": "self.version",
        "windwalker/database": "self.version",
        "windwalker/datamapper": "self.version",
        "windwalker/di": "self.version",
        "windwalker/dom": "self.version",
        "windwalker/environment": "self.version",
        "windwalker/event": "self.version",
        "windwalker/filesystem": "self.version",
        "windwalker/filter": "self.version",
        "windwalker/form": "self.version",
        "windwalker/html": "self.version",
        "windwalker/http": "self.version",
        "windwalker/io": "self.version",
        "windwalker/language": "self.version",
        "windwalker/loader": "self.version",
        "windwalker/middleware": "self.version",
        "windwalker/model": "self.version",
        "windwalker/profiler": "self.version",
        "windwalker/query": "self.version",
        "windwalker/record": "self.version",
        "windwalker/structure": "self.version",
        "windwalker/renderer": "self.version",
        "windwalker/router": "self.version",
        "windwalker/session": "self.version",
        "windwalker/string": "self.version",
        "windwalker/test": "self.version",
        "windwalker/uri": "self.version",
        "windwalker/utilities": "self.version",
        "windwalker/validator": "self.version",
        "windwalker/view": "self.version"
    }
}
